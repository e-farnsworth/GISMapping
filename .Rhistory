x <- runif(n, 0, 10)
beta0 <- 10
beta1 <- -3.33
beta2 <- -2.2
beta3 <- 2.22
x2 <- sample(c(1,0), n, replace=TRUE) #'x2' 3 way switch for sqrt2
pi_i <- exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x)/(1 + exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x))
y <- rbinom(n, 1, pi_i)
mydata <- data.frame(y, x)
myglm <- glm(y ~ x + x2 +x:x2, data=mydata, family="binomial")
summary(myglm)
b <- coef(myglm)
palette(c('green3', "firebrick"))
plot(y ~ x, data=mydata, pch=16, cex=2, col=as.factor(mydata$x2))
abline(h=0.5, v=5, lty=2, col="gray")
# curve 1
x2 = 0
curve(exp(b[1] + b[2]*x + b[3]*x2 + b[4]*x2*x)/(1+exp(b[1] + b[2]*x + b[3]*x2 + b[4]*x2*x)), add=TRUE, col="green3", lwd=2)
curve(exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x)/(1+exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x)), add=TRUE, col="green3", lty=2)
# curve 2
x2 = 1
curve(exp(b[1] + b[2]*x + b[3]*x2 + b[4]*x2*x)/(1+exp(b[1] + b[2]*x + b[3]*x2 + b[4]*x2*x)), add=TRUE, col="firebrick", lwd=2)
curve(exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x)/(1+exp(beta0 + beta1*x + beta2*x2 + beta3*x2*x)), add=TRUE, col="firebrick", lty=2)
legend("topleft", bty='n', legend=c("Estimated","True"), lty=c(1,2), col="green3")
